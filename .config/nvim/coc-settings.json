{
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.formatOnSaveFiletypes": [
        "json"
    ],
    "diagnostic.enable": false,
    "diagnostic.errorSign": "×",
    "diagnostic.warningSign": "»",
    "diagnostic.infoSign": "ღ",
    "diagnostic.hintSign": "ஐ",
    "diagnostic.displayByAle": false,
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextPrefix": " ❯❯❯ ",
    "diagnostic.enableMessage": "never",
    "diagnostic.messageTarget": "float",
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod"
            ],
            "filetypes": [
                "go"
            ]
        }
    },
    "git.enableGutters": true,
    "git.changedSign.hlGroup": "None",
    "git.removedSign.hlGroup": "None",
    "git.addedSign.hlGroup": "None",
    "git.topRemovedSign.hlGroup": "None",
    "git.changeRemovedSign.hlGroup": "None",
    "git.conflict.incoming.hlGroup": "None",
    "git.conflict.current.hlGroup": "None",
    "python.formatting.provider": "black",
    "snippets.ultisnips.directories": [
        "$HOME/.config/nvim/ultisnips"
    ]
}
