  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ]
    },
    "tinymist": {
      "command": "tinymist",
      "filetypes": [
        "typst"
      ],
      "settings": {
        "exportPdf": "onSave",
        "semanticTokens": "disable",
        "formatterMode": "typstyle"
      }
    }
  },
  "git.enableGutters": true,
  "git.conflict.incoming.hlGroup": "None",
  "git.conflict.current.hlGroup": "None",
  "git.floatConfig": {
    "border": true,
    "rounded": false
  },
  "python.linting.ruffEnabled": true,
  "semanticTokens.enable": false,
  "python.formatting.provider": "ruff",
  "pyright.organizeimports.provider": "ruff",
  "pyright.inlayHints.functionReturnTypes": true,
  "pyright.inlayHints.variableTypes": false,
  "python.formatting.autopep8Args": [
    "--max-line-length",
    "97"
  ],
  "python.formatting.blackArgs": [
    "--line-length",
    "97",
    "--preview"
  ],
  "diagnostic.level": "hint",
  "snippets.ultisnips.directories": [
    "$HOME/.config/nvim/ultisnips"
  ],
  "sumneko-lua.locale": "en-us",
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.checkUpdate": false,
  "Lua.runtime.version": "Lua 5.1",
  "Lua.workspace.checkThirdParty": false,
  "Lua.hint.arrayIndex": "Disable",
  "Lua.diagnostics.neededFileStatus": {
    "lowercase-global": "None"
  },
  "Lua.format.defaultConfig": {
    "quote_style": "double",
    "align_call_args": "false",
    "align_function_params": "false",
    "align_continuous_assign_statement": "false",
    "align_continuous_rect_table_field": "false",
    "align_array_table": "false"
  },
