{
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.formatOnSaveFiletypes": [
        "json"
    ],
    "suggest.defaultSortMethod": "none",
    "coc.preferences.diagnostic.refreshOnInsertMode": false,
    "diagnostic.enable": true,
    "diagnostic.errorSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.displayByAle": false,
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextPrefix": " ❯❯❯ ",
    "diagnostic.enableMessage": "never",
    "diagnostic.messageTarget": "echo",
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ]
        },
        "R": {
            "enable": true,
            "command": "/usr/bin/R",
            "args": [
                "--slave",
                "-e",
                "languageserver::run()"
            ],
            "filetypes": [
                "r"
            ]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": [
                "go.mod"
            ],
            "filetypes": [
                "go"
            ]
        },
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json"
            ],
            "filetypes": [
                "c",
                "cc",
                "cpp",
                "c++",
                "objc",
                "objcpp"
            ]
        }
    },
    "git.enableGutters": true,
    "git.changedSign.hlGroup": "None",
    "git.removedSign.hlGroup": "None",
    "git.addedSign.hlGroup": "None",
    "git.topRemovedSign.hlGroup": "None",
    "git.changeRemovedSign.hlGroup": "None",
    "git.conflict.incoming.hlGroup": "None",
    "git.conflict.current.hlGroup": "None",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length",
        "80",
        "--preview"
    ],
    "pyright.inlayHints.functionReturnTypes": true,
    "pyright.inlayHints.variableTypes": false,
    "snippets.ultisnips.directories": [
        "$HOME/.config/nvim/ultisnips"
    ],
    "sumneko-lua.locale": "en-us",
    "Lua.telemetry.enable": false,
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "Lua.hint.arrayIndex": "Disable",
    "Lua.diagnostics.neededFileStatus": {
        "lowercase-global": "None"
    },
    "suggest.completionItemKindLabels": {
        "constructor": "ﬥ",
        "keyword": "",
        "variable": "",
        "value": "",
        "operator": "Ψ",
        "function": "ƒ",
        "reference": "渚",
        "constant": "",
        "method": "",
        "struct": "פּ",
        "class": "",
        "interface": "",
        "text": "",
        "enum": "",
        "enumMember": "",
        "module": "",
        "color": "",
        "property": "",
        "field": "料",
        "unit": "",
        "event": "鬒",
        "file": "",
        "folder": "",
        "snippet": "",
        "typeParameter": "",
        "default": " "
    },
}
